{
    "name": "gemorroj/syntax",
    "type": "library",
    "description": "PHP code syntax checker.",
    "license": "LGPL-3.0",
    "require": {
        "php": "^7.1.3",
        "symfony/process": "^4.2|5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "authors": [
        {
            "name": "Gemorroj"
        }
    ],
    "autoload": {
        "psr-4": {
            "Syntax\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Syntax\\Tests\\": "tests/"
        }
    }
}
