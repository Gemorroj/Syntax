{
    "name": "gemorroj/syntax",
    "type": "library",
    "description": "PHP code syntax checker.",
    "license": "LGPL-3.0-or-later",
    "require": {
        "php": ">=7.3",
        "symfony/process": "^4.4|^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^2.18"
    },
    "authors": [
        {
            "name": "Gemorroj"
        }
    ],
    "autoload": {
        "psr-4": {
            "Syntax\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Syntax\\Tests\\": "tests/"
        }
    }
}
