{
    "name": "gemorroj/syntax",
    "type": "library",
    "description": "PHP code syntax checker.",
    "license": "LGPL-3.0-or-later",
    "require": {
        "php": ">=8.0.2",
        "symfony/process": "^5.4||^6.0||^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "friendsofphp/php-cs-fixer": "^3.41"
    },
    "authors": [
        {
            "name": "Gemorroj"
        }
    ],
    "autoload": {
        "psr-4": {
            "Syntax\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Syntax\\Tests\\": "tests/"
        }
    }
}
